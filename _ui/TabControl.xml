<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">	

    <Button name="CommonUI_Tab" virtual="true">
        <KeyValues>
            <KeyValue key="_autoHookHandlers" value="true" type="boolean"/>
        </KeyValues>
        <Scripts>
            <OnLoad>Vendor.LoadImplementation(self, "CommonUI", "CommandButton")</OnLoad>
        </Scripts>
        <Size x="64" y="20"/>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="text" inherits="GameFontNormal" justifyH="CENTER" maxLines="1" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="4" y="-4"/>
                        <Anchor point="BOTTOMRIGHT" x="-4" y="4"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Button>

    <Frame name="CommonUI_TabControl" virtual="true" mixin="BackdropTemplateMixin">
        <Scripts>
            <OnLoad>Vendor.LoadImplementation(self, "CommonUI", "TabControl")</OnLoad>
        </Scripts>
    </Frame>

</Ui>