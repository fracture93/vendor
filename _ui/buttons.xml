<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">	
    <Button name="CommandButton" virtual="true" mixin="BackdropTemplateMixin">
        <KeyValues>
            <KeyValue key="_autoHookHandlers" value="true" type="boolean"/>
        </KeyValues>
        <Scripts>
            <OnLoad>Vendor.LoadImplementation(self, "CommonUI", "CommandButton")</OnLoad>
        </Scripts>
        <Size x="120" y="28"/>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="CENTER" maxLines="1" justifyV="MIDDLE">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="16" y="-4"/>
                        <Anchor point="BOTTOMRIGHT" x="-16" y="4"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Button>
</Ui>